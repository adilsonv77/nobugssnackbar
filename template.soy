{/template}

/**
 * Toolbox.
 */
{template .toolbox}
  <xml id="toolbox" style="display: none">
    {if $ij.enabled['snackMan']}
	    <category name="{msg meaning="SnackMan.catSnackMan" desc="--"}SnackMan{/msg}">
	      {if $ij.enabled['goToBarCounter']}
	      <block type="move_goToBarCounter">
	        <value name="VALUE">
	          <block type="math_number">
	            <field name="NUM">1</field>
	          </block>
	        </value>
	      </block>
	      {/if}
	      {if $ij.enabled['isThereACustomer']}<block type="ask_isThereACustomer"> </block>{/if}
		  
		  {if $ij.enabled['askWantHowManyFoods']}<block type="ask_askWantHowManyFoods"> </block>{/if}
	      {if $ij.enabled['askHasHunger']}<block type="ask_askHasHunger"> </block>{/if}
	      {if $ij.enabled['askForFood']}<block type="ask_askForFood"> </block>{/if}
	      {if $ij.enabled['goToDisplay']}<block type="move_goToDisplay"> </block>{/if}
	      {if $ij.enabled['pickUpHotDog']}
	      <block type="prepare_pickUpHotDog"> 
	      	 <value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
			</value>
	      </block>
	      {/if}
	
		  {if $ij.enabled['askWantHowManyDrinks']}<block type="ask_askWantHowManyDrinks"> </block>{/if}
	      {if $ij.enabled['askHasThirsty']}<block type="ask_askHasThirsty"> </block>{/if}
	      {if $ij.enabled['askForDrink']}<block type="ask_askForDrink"> </block>{/if}
		  {if $ij.enabled['isTypeOfDrink']}<block type="compare_isTypeOfDrink"> </block>{/if}
	      
	      {if $ij.enabled['goToCooler']}<block type="move_goToCooler"> </block>{/if}
	      
	      {if $ij.enabled['goToBoxOfFruits']}<block type="move_goToBoxOfFruits"></block>{/if}
	       
	      {if $ij.enabled['pickUpFruits']}
	      <block type="prepare_pickUpFruits"> 
	      	<value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
			</value>
	      </block>
	      {/if}
	      {if $ij.enabled['goToJuiceMachine']}<block type="move_goToJuiceMachine"> </block>{/if}
	      {if $ij.enabled['prepareAndPickUpJuice']}
	      <block type="prepare_prepareAndPickUpJuice"> 
	      	<value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
			</value>
	      </block>
	      {/if}
	      
	      {if $ij.enabled['pickUpDrink']}
	      <block type="prepare_pickUpDrink"> 
	      	<value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
			</value>
	      </block>
	      {/if}
	      
	      {if $ij.enabled['deliver']}
	      <block type="do_deliver"> 
	      <value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
			</value>
	      </block>
	      {/if}

		  {if $ij.enabled['icecream']}
		   <block type="move_goToIceCreamMachine"> </block>
		   
		   <block type="prepare_pickUpIceCream">
		      <value name="VALUE">
				<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
		      </value>
		   </block>
		  {/if}

		  {if $ij.enabled['askForIceCream']}<block type="ask_askForIceCream"> </block>{/if}
		  {if $ij.enabled['askWantHowManyIceCream']}<block type="ask_askWantHowManyIceCream"> </block>{/if}
		  
	      {if $ij.enabled['wait']}
	      <block type="do_wait"> 
	      <value name="VALUE">
	          <block type="math_number">
	            <field name="NUM">5</field>
	          </block>
			</value>
	      </block>{/if}
	      
	    </category>
    {/if}
    
    {if $ij.enabled['money']}
	    <category name="{msg meaning="SnackMan.catMoney" desc="--"}Money{/msg}">
	    	{if $ij.enabled['cashIn']}
	    	<block type="money_cashIn"> 
		    	<value name="VALUE">
					<block type="variables_get">
						<field name="VAR">{{msg meaning="NoBugs.amountCharged" desc="---"}}amountCharged{{/msg}}</field>
					</block>
		    	</value>
	    	</block>
	    	{/if}
	    	
	    	{if $ij.enabled['giveChange']}
	    	<block type="money_giveChange"> 
		    	<value name="VALUE">
					<block type="variables_get">
						<field name="VAR">{{msg meaning="NoBugs.numberBankNotes" desc="---"}}numberOfBankNotes{{/msg}}</field>
					</block>
		    	</value>
		    	<value name="COINSBANKNOTES">
		    		<block type="const_money5"></block>
		    	</value>
	    	</block>
	    	
	    	<block type="const_money1"></block>
	    	<block type="const_money2"></block>
	    	<block type="const_money5"></block>
	    	<block type="const_money10"></block>
	    	<block type="const_money20"></block>
	    	{/if}

	    	{if $ij.enabled['money2']}
	    	<block type="money_giveChange"> 
		    	<value name="VALUE">
					<block type="variables_get">
						<field name="VAR">{{msg meaning="NoBugs.numberBankNotes" desc="---"}}numberOfBankNotes{{/msg}}</field>
					</block>
		    	</value>
		    	<value name="COINSBANKNOTES">
		    		<block type="const_money2"></block>
		    	</value>
	    	</block>
	    	
	    	<block type="const_money2"></block>
	    	{/if}
	    	
	    </category>
	{/if}
	
    {if $ij.enabled['vars']}
    <category name="{msg meaning="Apps.catVariables" desc="--"}Variables{/msg}" custom="VARIABLE"></category>
    {/if}
    
    {if $ij.enabled['text']}
    <category name="{msg meaning="Apps.catText" desc="--"}Text{/msg}">
        <block type="do_talk"> 
	         <value name="VALUE">
            	<block type="variables_get">
					<field name="VAR">{{msg meaning="NoBugs.order" desc="---"}}order{{/msg}}</field>
				</block>
          	 </value>
        </block>
  		
  		{if $ij.enabled['textjoin']}
    	<block type="text_join">
    		<mutation items="2"></mutation>
    		<value name="ADD0">
    			<block type="text">
    				<field name="TEXT">Hello</field>
    			</block>
    		</value>
    		<value name="ADD1">
    			<block type="text">
    				<field name="TEXT">World</field>
    			</block>
    		</value>
    	</block>
    	{/if}
    </category>
    {/if}

    {if $ij.enabled['logic']}
    <category name="{msg meaning="Apps.catLogic" desc="--"}Logic{/msg}">
      <block type="controls_if"></block>
       {if $ij.enabled['hide_logic_compare']}
       {else}
      <block type="logic_compare"></block>
       {/if}
      
      {if $ij.enabled['logic_operation']}
      	<block type="logic_operation"></block>
      {/if}
      
      {if $ij.enabled['logic_trueFalse']}
	      <block type="logic_negate"></block>
	      <block type="logic_boolean"></block>
      {/if}
    </category>
    {/if}

    {if $ij.enabled['const']}
    	<category name="{msg meaning="SnackMan.constSnackMan" desc="--"}Constants{/msg}">
    	 {if $ij.enabled['const.hotdog']}<block type="const_hotdog"> </block>{/if}
    	 {if $ij.enabled['const.juiceOfOrange']}<block type="const_juiceOfOrange"> </block>{/if}
    	 {if $ij.enabled['const.softDrink']}<block type="const_softDrink"> </block>{/if}
    	 {if $ij.enabled['const.chocolate']}<block type="const_chocolate"> </block>{/if}
    	 {if $ij.enabled['const.strawberry']}<block type="const_strawberry"> </block>{/if}
    	 {if $ij.enabled['const.vanilla']}<block type="const_vanilla"> </block>{/if}
        </category>
    {/if}
    	
    {if $ij.enabled['loop']}
    <category name="{msg meaning="Apps.catLoops" desc="--"}Loops{/msg}">
      {if $ij.enabled['whileUntil']}<block type="controls_whileUntil"></block>{/if}
      
      {if $ij.enabled['for']}
      <block type="controls_for">
        <field name="VAR">i</field>
        <value name="FROM">
          <block type="math_number">
            <field name="NUM">1</field>
          </block>
        </value>
        <value name="TO">
          <block type="math_number">
            <field name="NUM">10</field>
          </block>
        </value>
        <value name="BY">
          <block type="math_number">
            <field name="NUM">1</field>
          </block>
        </value>
      </block>
      {/if}
    </category>
    {/if}
    {if $ij.enabled['math']}
    <category name="{msg meaning="Apps.catMath" desc="--"}Math{/msg}">
          <block type="math_number"></block>
          <block type="math_arithmetic"></block>
    </category>
    {/if}
    {if $ij.enabled['function']}
    <category name="{msg meaning="Apps.catProcedures" desc="IBID"}Functions{/msg}" custom="PROCEDURE"></category>
    {/if}
  </xml>
{/template}

/**
 * Next button in information dialogs.
 */
{template .nextButton private="true"}
    <button id="nextexplanation" class="nobugs_button" onclick="Explanation.nextStatement()">
      {{msg meaning="Apps.dialogNext" desc="---"}}Next{{/msg}}
    </button>
{/template}
 
/**
 * Previous button in information dialogs.
 */
{template .previousButton private="true"}
    <button id="previousexplanation" class="nobugs_button" onclick="Explanation.previousStatement()">
      {{msg meaning="Apps.dialogPrevious" desc="---"}}Previous{{/msg}}
    </button>
{/template}
 
/**
 * Finish button in information dialogs.
 */
{template .finishButton private="true"}
    <button id="finishexplanation" class="nobugs_button" onclick="Explanation.finishStatement()">
      {{msg meaning="Apps.dialogOk" desc="---"}}OK{{/msg}}
    </button>
{/template}

/**
 * Logoff button in select mission dialog.
 */
{template .logoffDlgButton private="true"}
	<div class="farSide" >
	    <button id="logoffgame" class="secondary" onclick="Game.logoffButtonClick()">
	      {{msg meaning="Apps.dialogLogoff" desc="---"}}Logoff{{/msg}}
	    </button>
	</div>
{/template}

/**
 * Buy button in open mission dialog.
 */
{template .buyButton private="true"}
	<div class="farSide" >
	    <button class="nobugs_button" id="BuyMachine" onclick="Game.buyMachineButtonClick()" disabled="disabled">
	      {{msg meaning="NoBugs.buy" desc="---"}}Buy{{/msg}}
	    </button>
	    <button class="nobugs_button" onclick="BlocklyApps.hideDialog(false)">
	      {{msg meaning="Apps.close" desc="---"}}Close{{/msg}}
	    </button>
	</div>
{/template}
