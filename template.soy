{namespace nobugspage}

/**
 * This is a Closure Template.
 *
 * To regenerate just en.js, run this command:
 * java -jar ../_soy/SoyToJsSrcCompiler.jar --outputPathFormat generated/en.js --srcs ../common.soy,template.soy
 *
 * To regenerate all files, see: trunk/apps/common.soy
 */
{template .start}
  
  {call commonheader.start/}	
  {call importsjs.imports/}	
  
  <div id="initialBackground" style="display: none">
  	<div>
  		<form class="farSide" onsubmit="return false">
	  		<fieldset style="border: 0px none">
	  			<a href="#"onclick="changeLanguageToPt()" style="position: absolute; left: 8px"><img src="images/pt.png" alt="Portuguese Language" height="30" width="30"></a>
	  			<a href="#"onclick="changeLanguageToEn()" style="position: absolute; left: 40px"><img src="images/en.png" alt="Portuguese Language" height="30" width="30"></a>
	  			
	  			<label class="user">
	  				<input type="text" class="nobugs_inputs" id="loginuser" placeHolder="{msg meaning="NoBugs.user" desc="--"}User{/msg}"/>
	  			</label>
	  			<label class="password">
	  			    <input type="password" class="nobugs_inputs" id="loginpassw" placeHolder="{msg meaning="NoBugs.password" desc="--"}Password{/msg}"/>
	  			</label>
	  			<button id="ButtonLogin" class="nobugs_button" onclick="Game.login()">
	      			{{msg meaning="Apps.dialogLogin" desc="---"}}Login{{/msg}}
	    		</button>
	    		<button class="nobugs_button" onclick="location.href='javascript:void(0)'"  id="sign" style="display: none">
	    		{{msg meaning="Apps.dialogSignUp" desc="---"}}Sign Up{{/msg}}
	    		</button>
	  		</fieldset>
  		</form>
  		<div>
  			<div id="errorLogin" style="color: red; position: relative"></div>
  		</div>
  		<div id="suporte" style="position: relative">
  			<span id="suportespan">
  			{{msg meaning="NoBugs.support" desc="---"}}In case of technical problems, or reset password, please contact {{/msg}}<a href="mailto:adilsonv77@gmail.com">adilsonv77@gmail.com</a>
  			</span>
  		</div>
  	</div>
  	<div style="width: 100%"> 
	  	<table style="position: fixed; bottom: 0px; right: 0px;">
	  		<tr>
	  			<td colspan="2" align="center" style="padding-bottom: 5px"><i>{{msg meaning="NoBugs.sponsoredBy" desc="---"}}Sponsored by{{/msg}}</i></td><td colspan="2" align="center"><i>{{msg meaning="NoBugs.developedBy" desc="---"}}Developed by{{/msg}}</i></td>
	  		</tr>
	  		<tr>
	  			<td align="center" style="padding: 5px;"><img src="images/capes.png"/></td>
	  			<td align="center" style="padding: 5px; border-right: solid 1px black"><img src="images/udesc.png"/></td>
	  			<td align="center" style="padding: 5px;"><img src="images/uc.png"/></td>
	  			<td align="center" style="padding: 5px;"><img src="images/cms.png"/></td>
	  		</tr>
	  	</table>
  	</div>
  </div>

  {call commonheader.messages /}
  
  <div id="selectMission" style="display: none">
  	  <table width="100%" class="nobugs_background" id="topInfoTable">
	    <tr>
	      <td rowspan="2" style="width: 200px">
	          <span id="selectMissionTitle">
	           <img src="images/logotipo.png" height="60px" width="185px"/>
	          </span>
	      </td>
	      <td id="playerName" class="nobugs_font" style="width:80%">
	      </td>
	      <td rowspan="2" class="farSide">
	         <div>
	  	     	 <button id="selectMissionLogoffButton" style="background-color: transparent; border: 0px" title="{msg meaning="NoBugs.logoffTooltip" desc="--"}Logoff the game.{/msg}">
		        	<img id="logoffGame" src="images/logoff.png" style="width:32px; height:32px"/>
		         </button>
	         </div>
	      </td>
	    </tr>
	    <tr>
	      <td>
	      	<div class="playerReward">
	      		<div class="playerRewardIcon" style="background-image: url('images/xp.png');"></div>
  				<span id="yourXP"></span>
	      	</div>
	      	<div class="playerReward">
	      		<div class="playerRewardIcon" style="background-image: url('images/coin2.png');"></div>
	      		<span id="yourCash"></span>
	      	</div>
	      </td>
	    </tr>
	  </table>
  
  	  <div id="playerInfo" class="inverse_nobugs_background" style="text-align: center">
  	  
      	<canvas id="avatarPlayer" width="72" height="116"></canvas>
      	<button id="avatarEditorButton" class="nobugs_button" style="font-size: 14px; width: 100px"  title="{msg meaning="NoBugs.avatarTooltip" desc="--"}Change your avatar.{/msg}">
      	Avatar
