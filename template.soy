      	<button id="snackBarEditorButton" class="nobugs_button" style="font-size: 14px; width: 100px" disabled="disabled" title="{msg meaning="NoBugs.yourSnackBarTooltip" desc="--"}Change the snack bar that you are the owner.{/msg}">
      	Snack Bar
      	</button>

      	<button id="profileEditorButton"  class="nobugs_button" style="font-size: 14px; width: 100px" title="{msg meaning="NoBugs.profileTooltip" desc="--"}Change your profile.{/msg}">
      	{{msg meaning="NoBugs.Profile" desc="---"}}Profile{{/msg}}
      	</button>
      	
  	  </div>
  
  
  	  <table id="tbSelectMission" style="margin: auto">
  		<tr>
  			<td id="tdSelectMission" style="height: 342px">
  				
		 		<div id="schoolMap">
					<canvas id="city_map" width="400" height="300"/>
				</div>
  			</td>
  			
  			<td style="height: 342px">
				<div id="leaderboard">
					<ul id='leaderboard_tabs'>
						<li id="leaderpoints" class='leaderboard_tab' data-tooltip-position="top center"><a href="#tabs-points"><img src="images/badge_points_gold.png"/></a></li>
						<li id="leadertime" class='leaderboard_tab' data-tooltip-position="top center"><a href="#tabs-time"><img src="images/badge_time_gold.png"/></a></li>
						<li id="leaderruns" class='leaderboard_tab' data-tooltip-position="top center"><a href="#tabs-runs"><img src="images/badge_runs_gold.png"/></a></li>
